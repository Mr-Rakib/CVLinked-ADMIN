
@if (TempData["DeleteError"] != null)
{

    <div class="alert alert-delete" role="alert">
        @TempData["DeleteError"].ToString()
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
}
