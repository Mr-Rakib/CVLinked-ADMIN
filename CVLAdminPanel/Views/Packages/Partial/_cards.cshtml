@{ 
    int totalSubscriber = new DashboardService().TotalPackageSubscriber(Context.User.FindFirstValue("access_token"));
}
<div class="col-xl-12 col-lg-12">
    <div class="card card-stats mb-4 mb-xl-0">
        <div class="card-body">
            <div class="row">
                <div class="col">
                    <h5 class="card-title text-uppercase text-muted mb-1">TOTAL SUBSCRIBER</h5>
                    <span class="h2 font-weight-bold mb-0 ">
                        @totalSubscriber
                    </span>
                </div>
                <div class="col-auto">
                    <div class="icon icon-shape bg-danger text-white rounded-circle shadow">
                        <i class="fas fa-user-tie"></i>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>